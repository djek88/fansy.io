<body class="body">
  <div class="s-header">
    <div class="promo-container w-container"><a class="link-block-logo w-inline-block" href="/"><h1 class="text-logo-small">fansy.ai</h1></a>
      <div class="header-nav-right"><a class="header-nav promo-nav" href="mailto:kk@fansy.io">Contact Us</a><a class="join-btn join-header-btn w-button" href="#">Join Private Beta</a>
      </div>
    </div>
  </div>
  <div class="section">
    <div class="c-content w-container">
      <div class="b-header">
        <div class="text-breadcrumbs">{{name}}</div>
        <div class="b-page-nav">
          <!--a class="user-nav-active w-inline-block" href="#">
            <div>GAMES</div>
          </a-->
          <!--a class="user-nav w-inline-block" href="highlights.html">
            <div class="text-block-4">HIGHLIGHTS</div>
          </a-->
        </div>
      </div>

      {{#each gamesList}}
        <div class="row-grid w-row">
        {{#each item}}
          <div class="w-col w-col-4">
            <a class="w-inline-block" href="{{url}}"><img class="image-game" src="{{thumb}}"></a>
            <div class="text-grid-title">
              <a href="{{url}}" class="link-h">{{heroName}}</a> <!--{{kill}}/{{death}}/{{assists}}-->
            </div>
            <div class="text-grid-subtitle">{{gameDate}}, <strong>{{hlNum}}</strong> highlights</div>
          </div>
        {{/each}}
        </div>
      {{/each}}

    </div>
  </div>

  {{#if author}}
  <div class="c-pagination w-container">
    
    {{#each pagesBefore}}
        <a class="link-pagination w-inline-block" href="?page={{this}}">
          <div>{{this}}</div>
        </a>
    {{/each}}

    <a class="link-pagination-selected w-inline-block" href="#">
      <div class="text-block-6">{{currentPage}}</div>
    </a>

    {{#each pagesAfter}}
        <a class="link-pagination w-inline-block" href="?page={{this}}">
          <div>{{this}}</div>
        </a>
    {{/each}}

